.search_input {
  height: 80rpx;
  padding: 10rpx 15px;
  box-shadow: 0px 5px 5px 0px rgba(0, 0, 0, 0.05);
}
.search_input navigator {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #EEEEEE;
  border-radius: 15px;
  color: #999999;
  font-size: 26rpx;
  letter-spacing: 1px;
}
.search_input navigator image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10px;
}
