@import "./styles/iconfont.wxss";

/* 在微信小程序中 不支持 通配符 '*' */
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/*
 主题颜色 通过变量来实现 
 1 less 中 存在 变量这个知识
 2 原生的css和wxss也是支持 变量 
 */
 page{
   /* 定义主题颜色 */
   --themeColor:#FE5E56;
   /*
    定义统一字体大小  假设设计稿 大小是 375px 
    1px= 2rpx
    14px = 28rpx
     */
     font-size: 28rpx;
 }

 image{
   width: 100%;
 }

 
 @font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1540364458716');
  /* IE9*/
  src: url('iconfont.eot?t=1540364458716#iefix') format('embedded-opentype'),
    /* IE6-IE8 */
    url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
    url('iconfont.ttf?t=1540364458716') format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('iconfont.svg?t=1540364458716#iconfont') format('svg');
  /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-jiantou:before {
  content: "\e616";
}